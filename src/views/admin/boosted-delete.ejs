<%- include('static/head.ejs', { bot,user,path}) %>
<div id="content" class="main-content">
    <br><br><br><br><br><br>
    <div class="container">
    <form method="POST">
      <input type="hidden" value="<%= _token %>" name="_token" id="_token">

        <div class="form-group mb-4">
            <label for="formGroupExampleInput">Razón</label>
            <input type="text" class="form-control" id="reason" name="reason" placeholder="razón" required>
        </div>
        <input type="submit" name="time" class="btn btn-primary" value="Submit">
    </form>
</div>
<br><br><br><br><br><br><br><br><br><br><br><br><br>
<script>
    var urlParams = new URLSearchParams(window.location.search);
if(urlParams.has('success') && urlParams.get('success')){
Swal.fire({
 icon: 'success',
 title: 'Exitoso...',
 closeOnConfirm: false,
 text: urlParams.get('message'),
 footer: 'Copyright 2021 &copy BotCity - Todos los derechos reservados.'
}).then((result) => {
if (result.isConfirmed) {
window.location.href = '/admin/boosted-bots';
}
})
}
if(urlParams.has('error') && urlParams.get('error')){
Swal.fire({
 icon: 'error',
 title: 'Oops... Ha ocurrido un error',
 text: urlParams.get('message'),
 footer: 'Copyright 2021 &copy BotCity - Todos los derechos reservados.'
}).then((result) => {
if (result.isConfirmed) {
window.location.href = '/admin/boosted-bots';
}    
})
}
</script>
</div></div>
<%- include('static/foot.ejs', { bot,user,path}) %>